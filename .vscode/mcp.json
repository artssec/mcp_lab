{
	"servers": {
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest",
        "--proxy-server=http://127.0.0.1:8080"
			],
			"type": "stdio"
		},
		"filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/phara/HackingDay2025/mcp-lab",
        "/home/phara/Tools/shcheck"
      ]
    },
		"burp": {
      "command": "/usr/bin/java",
      "args": [
          "-jar",
          "/home/phara/HackingDay2025/mcp-proxy.jar",
          "--sse-url",
          "http://127.0.0.1:9876"
      ]
    }
	},
	"inputs": []
}